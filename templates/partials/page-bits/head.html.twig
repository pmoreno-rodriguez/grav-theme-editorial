<!-- HEAD METADATA -->
<meta charset="utf-8" />
<title>{% if header.title %}{{ header.title|e('html') }} | {% endif %}{{ site.title|e('html') }}</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes" />
{% include 'partials/page-bits/metadata.html.twig' %}
{% if favicon %}
{% set favicon_file = (favicon|first).name %}
{% set favicon_type = (favicon|first).type %}
<link rel="icon" href="{{ url('theme://images/' ~ favicon_file) }}" type="{{ favicon_type }}" />
{% else %}
<link rel="icon" href="{{ url('theme://images/logo.png') }}" type="image/png" />
{% endif %}
<link rel="canonical" href="{{ page.url(true, true) }}" />
